  t = interval-int(millis()/1000);
  time = nf(t, 3);
  
  if (t == 0) {
    stop();
    text("GAME OVER", 490, 360);
    textSize(70);
    
    //interval +=16;
  }
  
  text("time",width/12, height/8);
  text(time, width/12, height/4);
  textSize(70);


PFont font;
String time = "0:16";
int t;
int interval = 16;
